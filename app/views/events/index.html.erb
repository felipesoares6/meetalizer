<% @events.each do |event| %>
    <p> Name: <%= event.name %> </p>
    <p> Description: <%= event.description %> </p>
    <p> Address: <%= event.address %> </p>
    <p> Start Date: <%= event.start_date %> </p>
    <p> End Date: <%= event.end_date %> </p>
<% end %>

<% unless @events.exists? %>
  <p> No events yet! </p>
<% end %>

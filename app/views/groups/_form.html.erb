<%= form_for :group, url: url, method: method, data: { test: "#{type}_group_form" } do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :region %>
  <%= f.text_field :region %>

  <%= f.label :profile_picture_url %>
  <%= f.url_field :profile_picture_url %>

  <%= f.label :cover_picture_url %>
  <%= f.url_field :cover_picture_url %>

  <%= f.submit %>
<% end %>
